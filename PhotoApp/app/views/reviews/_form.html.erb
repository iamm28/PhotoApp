<%= form_for @review, html: { multipart: true } do |f| %>

  <%= f.label :like %><br />
  <%= f.check_box :like, class: "form-control" %><br />

  <%= f.label :comment %><br />
  <%= f.text_field :comment, class: "form-control" %><br />

  <%= f.label :user %><br />
  <%= f.collection_select :user_id, @users, :id, :name, prompt: true %><br />

  <%= f.submit %>
<% end %>

